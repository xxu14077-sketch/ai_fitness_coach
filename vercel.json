{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
    { "src": "/icons/(.*)", "dest": "/public/icons/$1" },
    { "src": "/canvaskit/(.*)", "dest": "/public/canvaskit/$1" },
    { "src": "/(.*)\\.(.*)", "dest": "/public/$1.$2" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}